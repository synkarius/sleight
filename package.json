{
  "name": "sleight",
  "description": "voice commands construction gui util",
  "author": "synkarius",
  "build": {
    "appId": "com.explosionduck.sleight"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/mustache": "^4.2.1",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "bootstrap": "^5.1.3",
    "brandi": "^4.0.2",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "mustache": "^4.2.0",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-bootstrap-icons": "^1.8.2",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "electron": "^18.2.3",
    "electron-builder": "^23.0.3",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom --watchAll",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run build && electron-builder --linux --win --mac --x64 --ia32 --publish=never",
    "build": "npm run react-build && npm run electron-build --publish=never",
    "electron-start": "wait-on -v -i=1000 http://127.0.0.1:3000 && electron .",
    "start": "concurrently -k \"cross-env BROWSER=none npm run react-start\" \"npm run electron-start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}